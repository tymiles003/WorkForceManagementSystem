 <script>
                $(document).ready(function () {
                    $("#schedule").kendoGrid({
                        dataSource: {
                            type: "json",
                            transport: {
                                read: "/viewGuardSchedule"
                            },
                            pageSize: 20
                        },
                        height: 550,
                        groupable: true,
                        sortable: true,
                        filterable:true,
                        pageable: {
                            refresh: true,
                            pageSizes: true,
                            buttonCount: 5
                        },
                        columns: [{
                            field: "buildingName",
                            title: "Building Name",
                            width: 200
                        }, {
                            field: "startTime",
                            title: "Start Time",
                           template:"#= kendo.toString(new Date(startTime).toLocaleString(), 'MM/dd/yyyy HH:mm:ss')#"
                             }, {
                           field: "endTime",
                            title: "End Time",
                             template:"#= kendo.toString(new Date(endTime).toLocaleString(), 'MM/dd/yyyy HH:mm:ss')#"
                             }, {
                         field: "addressLine1",
                            title: "Address Line1"
                        }, {
                            field: "addressLine2",
                            title: "Address Line2"
                        }, 
                         {
                            field: "city",
                            title: "city"
                        },
                        {
                            field: "zipCode",
                            width: 150
                        }]
                    });
                });
            </script>

