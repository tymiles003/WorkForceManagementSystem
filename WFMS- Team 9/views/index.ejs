    <script type="text/javascript">
      function initialize() {
		var i=0;	 
        var mapCanvas = document.getElementById('map-canvas');
			var mapOptions = {
			  center: new google.maps.LatLng(37.330810, -121.882750),
			  zoom:17,
			  mapTypeId: google.maps.MapTypeId.HYBRID
			}
		var map = new google.maps.Map(mapCanvas, mapOptions)					 
		 var marker1=new google.maps.Marker({
		  position:new google.maps.LatLng(37.335353, -121.883318),
		  animation:google.maps.Animation.BOUNCE  
		  });
		  marker1.setMap(map);
		var marker2=new google.maps.Marker({
		  position:new google.maps.LatLng(37.335187, -121.881072),
		  animation:google.maps.Animation.BOUNCE  
		  });
		 marker2.setMap(map);
		 var marker3=new google.maps.Marker({
		  position:new google.maps.LatLng( 37.336146,  -121.882465),
		  animation:google.maps.Animation.BOUNCE  
		  });
		  marker3.setMap(map);
		  var marker4=new google.maps.Marker({
		  position:new google.maps.LatLng(  37.335492,   -121.884925),
		  animation:google.maps.Animation.BOUNCE  
		  });
		  marker4.setMap(map);
		  
		var infowindow1 = new google.maps.InfoWindow({
		  content:"Tower Hall!"
		  });
		  var infowindow2 = new google.maps.InfoWindow({
		  content:"SJSU Event Center!"
		  });
		   var infowindow3 = new google.maps.InfoWindow({
		  content:"Clark Hall!"
		  });
		   var infowindow4 = new google.maps.InfoWindow({
		  content:"Martin Luther King Library!"
		  });
		infowindow1.open(map,marker1);
		infowindow2.open(map,marker2);
		infowindow3.open(map,marker3);
		infowindow4.open(map,marker4);
      }
      google.maps.event.addDomListener(window, 'load', initialize);
    </script>
